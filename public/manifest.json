{
  "name": "Awesome decentralized server extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Awesome decentralized server extension",
  "icons": {
    "512": "logo512.png"
  },
  "permissions": ["unlimitedStorage"],
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}