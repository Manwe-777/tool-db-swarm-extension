{
  "name": "extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "crypto-browserify": "^3.12.0",
    "npm-watch": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "stream-browserify": "^3.0.0",
    "tool-db": "^1.2.3",
    "trystero": "^0.10.0",
    "typescript": "^4.5.4",
    "util": "^0.12.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "craco start",
    "build:app": "react-scripts build",
    "build:background": "craco build --config craco.background.config.js",
    "build": "set INLINE_RUNTIME_CHUNK=false && set GENERATE_SOURCEMAP=false && npm run build:background && npm run build:app && node after-build.js",
    "test": "craco test",
    "eject": "craco eject"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
