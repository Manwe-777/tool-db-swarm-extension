{
  "name": "extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "npm-watch": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "tool-db": "^1.1.13",
    "trystero": "^0.10.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "react-scripts start",
    "build:background": "tsc",
    "build": "set INLINE_RUNTIME_CHUNK=false && set GENERATE_SOURCEMAP=false && react-scripts build && node after-build.js && npm run build:background",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
